msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 12px;"
"\">\n"
"                        Hello,\n"
"                        <br/><br/>\n"
"                        % set transaction = object."
"get_portal_last_transaction()\n"
