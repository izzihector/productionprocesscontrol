"                                    ${format_amount(line."
"price_reduce_taxexcl, object.currency_id)}\n"
"                                    % else\n"
"                                    ${format_amount(line."
"price_reduce_taxinc, object.currency_id)}\n"
"                                    % endif\n"
"                                </strong></td>\n"
"                            </tr>\n"
"                        </table>\n"
"                        % endif\n"
